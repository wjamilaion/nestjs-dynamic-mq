  "bugs": {
    "url": "https://github.com/aiondigitalengineering/nestjs-dynamic-mq/issues"
  },
  "homepage": "https://github.com/aiondigitalengineering/nestjs-dynamic-mq#readme",
  "devDependencies": {
    "@nestjs/common": "10.1.3",
    "@nestjs/core": "10.1.3",
    "@nestjs/testing": "^10.1.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
